<!-- PROFILE CONTAINER -->
    <div class="profile-box row">
        <div class="picture col-md-3">
            <img class="profile-picture" src= "/images/<%= profileImg %>"  alt="profileImage">
        </div>
        <div class="posts col-md-3">
            <p  class="numbers">5</p>
            <h2 class="attribute">Posts</h2>
        </div>
        <div class="followers col-md-3">
          <p class="numbers">178</p>
          <h2 class="attribute">Followers</h2>
        </div>
        <div class="following col-md-3">
          <p class="numbers">325</p>
          <h2 class="attribute">Following</h2>
        </div>
        <div class="col-md-12 follow">
          <button type="button" class="btn btn-info button"  name="button">Follow</button>
        </div>
    </div>

<!-- BIO -->
    <div class="bio-box">


    <h2 class="username" > <%= username %> </h2>
    <p class="bio"> <%= bio %></p>



    </div>

